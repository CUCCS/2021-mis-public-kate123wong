
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:0c.0/usb1/1-1/1-1:1.0'
	option hwmode '11g'
	option txpower '20'
	option channel '12'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenWrtKate'
	option network 'wan lan'
	option key '12345678'
	option encryption 'psk+ccmp'

